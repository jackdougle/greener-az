{
  "name": "arizona-energy-map",
  "version": "1.0.0",
  "description": "Interactive heat map of electricity usage in Arizona for sustainability decisions",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"vite\" --names \"API,WEB\" --prefix name",
    "dev:frontend": "vite",
    "dev:backend": "npm run server",
    "server": "cd server && export $(cat ../.env.local | xargs) && npm start",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.20",
    "@types/node": "^24.5.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "concurrently": "^9.2.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.9.2",
    "vite": "^7.1.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackdougle/sunhacks-2025.git"
  },
  "keywords": [
    "arizona",
    "electricity",
    "sustainability",
    "heat-map",
    "energy"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jackdougle/sunhacks-2025/issues"
  },
  "homepage": "https://github.com/jackdougle/sunhacks-2025#readme"
}
